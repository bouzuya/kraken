{
  "name": "@bouzuya/kraken",
  "description": "A blog data generator for blog.bouzuya.net.",
  "version": "3.6.1",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ],
      "power-assert"
    ]
  },
  "bin": {
    "kraken": "./bin/kraken"
  },
  "bugs": {
    "url": "https://github.com/bouzuya/kraken/issues"
  },
  "dependencies": {
    "commander-b": "1.0.0",
    "finalhandler": "0.5.0",
    "fs-extra": "0.30.0",
    "jekyll-markdown-parser": "0.1.3",
    "kuromoji": "0.1.1",
    "marked": "0.3.5",
    "serve-static": "1.11.1",
    "time-keeper": "0.2.2"
  },
  "devDependencies": {
    "@types/finalhandler": "0.0.29",
    "@types/fs-extra": "0.0.29",
    "@types/marked": "^0.3.0",
    "@types/node": "6.0.33",
    "@types/power-assert": "^1.4.29",
    "@types/proxyquire": "^1.3.27",
    "@types/serve-static": "1.7.31",
    "@types/sinon": "^2.3.3",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "beater": "^5.0.1",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.0.2",
    "power-assert": "^1.4.4",
    "proxyquire": "^1.8.0",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.0",
    "typescript": "^2.4.2",
    "watch": "^1.0.2"
  },
  "directories": {
    "doc": "doc"
  },
  "files": [
    "bin",
    "lib"
  ],
  "homepage": "https://github.com/bouzuya/kraken",
  "keywords": [],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/bouzuya/kraken.git"
  },
  "scripts": {
    "build": "npm-run-all -s \"build:tmp:ts\" \"build:tmp:es2015\" -p \"build:lib:*\"",
    "build:lib:dts": "cpx \".tmp/es2015/src/**/*.d.ts\" \"lib/\"",
    "build:lib:js": "cpx \".tmp/es5/src/**/*.js\" \"lib/\"",
    "build:tmp:es2015": "babel --quiet .tmp/es2015/ -d .tmp/es5/",
    "build:tmp:ts": "tsc",
    "clean": "rimraf .tmp lib",
    "prepublish": "npm-run-all -s \"clean\" \"build\"",
    "test": "node .tmp/es5/test/index",
    "watch": "npm-run-all -p \"watch:**\"",
    "watch:test": "watch --wait 2 \"npm test\" \".tmp/es5/\"",
    "watch:tmp:es2015": "npm run \"build:tmp:es2015\" -- --watch",
    "watch:tmp:ts": "npm run \"build:tmp:ts\" -- --watch"
  }
}
