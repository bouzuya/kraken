{
  "name": "kraken",
  "description": "A blog data generator for blog.bouzuya.net.",
  "version": "2.1.3",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "bin": {
    "kraken": "./bin/kraken"
  },
  "bugs": {
    "url": "https://github.com/bouzuya/kraken/issues"
  },
  "dependencies": {
    "commander-b": "0.2.0",
    "es6-promise": "^2.1.1",
    "fs-extra": "^0.18.3",
    "marked": "0.3.2",
    "moment": "^2.10.3",
    "myjekyll": "https://github.com/bouzuya/myjekyll/archive/2.0.0.tar.gz"
  },
  "devDependencies": {
    "coffee-script": "1.10.0",
    "rimraf": "2.5.4",
    "uglify-js": "2.6.4"
  },
  "homepage": "https://github.com/bouzuya/kraken",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/bouzuya/kraken.git"
  },
  "scripts": {
    "build": "npm run build:coffee && npm run build:minify",
    "build:coffee": "coffee -b -c -o .tmp src",
    "build:minify": "([ -d lib ] || mkdir lib) && for f in .tmp/*.js; do uglifyjs --compress --mangle -o lib/\"${f##*/}\" -- \"${f}\"; done",
    "clean": "rimraf .tmp lib"
  }
}
