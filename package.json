{
  "name": "@bouzuya/kraken",
  "description": "A blog data generator for blog.bouzuya.net.",
  "version": "3.7.6",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 6
          }
        }
      ],
      "power-assert"
    ]
  },
  "bin": {
    "kraken": "./bin/kraken"
  },
  "bugs": {
    "url": "https://github.com/bouzuya/kraken/issues"
  },
  "dependencies": {
    "commander-b": "1.0.0",
    "finalhandler": "^1.1.1",
    "fs-extra": "^7.0.1",
    "jekyll-markdown-parser": "^1.1.1",
    "kuromoji": "^0.1.2",
    "marked": "^0.5.1",
    "serve-static": "^1.13.2",
    "time-keeper": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@types/finalhandler": "0.0.33",
    "@types/fs-extra": "^5.0.4",
    "@types/marked": "^0.4.2",
    "@types/node": "^10.12.9",
    "@types/power-assert": "^1.5.0",
    "@types/proxyquire": "^1.3.27",
    "@types/serve-static": "^1.13.2",
    "@types/sinon": "^5.0.6",
    "babel-preset-power-assert": "^3.0.0",
    "beater": "^5.0.1",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.3",
    "power-assert": "^1.6.1",
    "proxyquire": "^2.1.0",
    "rimraf": "^2.6.1",
    "sinon": "^7.1.1",
    "typescript": "^3.1.6",
    "watch": "^1.0.2"
  },
  "directories": {
    "doc": "doc"
  },
  "files": [
    "bin",
    "lib"
  ],
  "homepage": "https://github.com/bouzuya/kraken",
  "keywords": [],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/bouzuya/kraken.git"
  },
  "scripts": {
    "build": "npm-run-all -s \"build:tmp:ts\" \"build:tmp:es2015\" -p \"build:lib:*\"",
    "build:lib:dts": "cpx \".tmp/es2015/src/**/*.d.ts\" \"lib/\"",
    "build:lib:js": "cpx \".tmp/es5/src/**/*.js\" \"lib/\"",
    "build:tmp:es2015": "babel .tmp/es2015/ -d .tmp/es5/",
    "build:tmp:ts": "tsc",
    "clean": "rimraf .tmp lib",
    "prepare": "npm-run-all -s \"clean\" \"build\"",
    "test": "node .tmp/es5/test/index",
    "watch": "npm-run-all -p \"watch:**\"",
    "watch:test": "watch --wait 2 \"npm test\" \".tmp/es5/\"",
    "watch:tmp:es2015": "npm run \"build:tmp:es2015\" -- --watch",
    "watch:tmp:ts": "npm run \"build:tmp:ts\" -- --watch"
  }
}
